<div class="outer-container">
  <h1>{{model.event.name}}</h1>
  <img src={{model.event.image_url}} alt={{model.event.name}} data-photo-id={{model.event.slug}} />
  <p>{{model.event.summary}}</p>
  <ul>
    <li>Time: {{moment-format model.event.start_time 'ddd MMM DD, h:mma'}}</li>
    <li>Location: <div id="asdf">{{model.event.venue}}</div></li>
    <li>Cost: {{model.event.cost}}</li>
    <li></li>
  </ul>

  {{#if model.event.venue}}
    {{google-map venue=model.event.venue}}
  {{/if}}
</div>
